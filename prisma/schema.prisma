datasource db {
  provider = "sqlite"
  url      = "file:../data/manga.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  createdAt DateTime @default(now())
}

model LibraryPath {
  id        Int      @id @default(autoincrement())
  path      String
  createdAt DateTime @default(now())
}

